	"private": false,
	"description": "VS Code part of language server",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandPowerquery#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandPowerquery/issues"
	},
	"repository": {
